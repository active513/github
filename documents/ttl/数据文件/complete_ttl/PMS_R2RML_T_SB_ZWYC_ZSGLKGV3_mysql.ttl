	    T_SB_ZWYC_ZSGLKG.OBJ_ID,
		(
		CASE T_SB_ZWYC_ZSGLKG.ZCXZ
		WHEN '01' THEN
			'机构类别(国家电网公司)'
		WHEN '02' THEN
			'机构类别(分部)'
		WHEN '03' THEN
			'机构类别(省-直辖市-自治区公司)'
		WHEN '04' THEN
			'机构类别(子公司)'	
		WHEN '05' THEN
