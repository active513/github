<#T_SB_ZNYC_PDBYQ_Map_lifeCycleState>
	    rr:logicalTable [ rr:sqlQuery """
        SELECT
	    PMS_SCYW.T_SB_ZNYC_PDBYQ.OBJ_ID,
	(
		CASE PMS_SCYW.T_SB_ZNYC_PDBYQ.YXZT
		WHEN CAST ('30' AS NVARCHAR2(2)) THEN
			'资产全寿命状态(退役)'
		WHEN CAST ('40' AS NVARCHAR2(2)) THEN
			'资产全寿命状态(报废)'
		WHEN CAST ('33' AS NVARCHAR2(2)) THEN
			'资产全寿命状态(待报废)'	
		END
	) LIFECYCLE

		FROM PMS_SCYW.T_SB_ZNYC_PDBYQ WHERE YXZT='30' OR YXZT='40' OR YXZT='33' 
		""" ];
 	rr:subjectMap [
		rr:template "http://nari.com/CDPSM/CIM_DEMO/T_SB_ZNYC_PDBYQ(OBJ_ID)#{OBJ_ID}";
		rr:termType rr:IRI;
	];
	
	
	rr:predicateObjectMap [
		rr:predicate cim:Asset.lifecycleState;
		rr:objectMap [ 
			rr:template "http://nari.com/CDPSM/CIM_DEMO/T_SB_ZNYC_PDBYQ(YXZT)#{LIFECYCLE}";
			rr:termType rr:Literal;
		];
	].
	
	
<#T_SB_ZNYC_PDBYQ_Map_Type>
	    rr:logicalTable [ rr:sqlQuery """
        SELECT
	    PMS_SCYW.T_SB_ZNYC_PDBYQ.OBJ_ID,
		(
		CASE PMS_SCYW.T_SB_ZNYC_PDBYQ.ZCXZ
		WHEN CAST ('01' AS NVARCHAR2(2)) THEN
			'机构类别(国家电网公司)'
		WHEN CAST ('02' AS NVARCHAR2(2)) THEN
			'机构类别(分部)'
		WHEN CAST ('03' AS NVARCHAR2(2)) THEN
			'机构类别(省-直辖市-自治区公司)'
		WHEN CAST ('04' AS NVARCHAR2(2)) THEN
			'机构类别(子公司)'	
		WHEN CAST ('05' AS NVARCHAR2(2)) THEN
			'机构类别(用户)'
		WHEN CAST ('06' AS NVARCHAR2(2)) THEN
			'机构类别(代管县)'	
		END
	) TYPE
	
		FROM PMS_SCYW.T_SB_ZNYC_PDBYQ WHERE  ZCXZ = '01' OR  ZCXZ = '02' OR  ZCXZ = '03' OR  ZCXZ = '04' OR  ZCXZ = '05' OR  ZCXZ = '06'
		
		""" ];
 	rr:subjectMap [
		rr:template "http://nari.com/CDPSM/CIM_DEMO/T_SB_ZNYC_PDBYQ(OBJ_ID)#{OBJ_ID}";
		rr:termType rr:IRI;
	];
	
	
	rr:predicateObjectMap [
		rr:predicate cim:Asset.type;
		rr:objectMap [ 
			rr:template "http://nari.com/CDPSM/CIM_DEMO/T_SB_ZNYC_PDBYQ(ZCXZ)#{TYPE}";
			rr:termType rr:Literal;
		];
	].


