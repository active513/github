<#T_SB_ZNYC_PDBYQ_Map_AssetDEPLOYMENT_1>
	rr:logicalTable [ rr:sqlQuery """
    SELECT
	PMS_SCYW.T_SB_ZNYC_PDBYQ.YXZT, PMS_SCYW.T_SB_ZNYC_PDBYQ.OBJ_ID ,PMS_SCYW.T_SB_ZNYC_PDBYQ.TYRQ
	FROM PMS_SCYW.T_SB_ZNYC_PDBYQ WHERE YXZT='31' OR YXZT='10' OR YXZT='20' OR T_SB_ZNYC_PDBYQ.TYRQ IS NOT NULL """ ];
 	rr:subjectMap [
		rr:template "http://nari.com/CDPSM/CIM_DEMO/T_SB_ZNYC_PDBYQ(OBJ_ID)#{OBJ_ID}";
		rr:termType rr:IRI;
	];
	
	
	rr:predicateObjectMap [
		rr:predicate cim:Asset.AssetDeployment;
		rr:objectMap [ 
			rr:template "http://nari.com/CDPSM/CIM_DEMO#{OBJ_ID}_AssetDeployment";
		 	rr:termType rr:IRI;
		];
	];
	
	rr:predicateObjectMap [
		rr:predicate cim:Asset.lifecycleState;
		rr:objectMap [ 
			rr:constant "资产全寿命状态(接收)_补充";
		 	rr:termType rr:Literal;
		];
	];
	
	rr:predicateObjectMap [
		rr:predicate cim:Asset.inUseState;
		rr:objectMap [ 
			rr:constant "使用状态(在使用)_补充";
			rr:termType rr:Literal;
		];
	].

