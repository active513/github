<#T_SB_ZNYC_PDBYQ_Map_ASSET_DEPLOYMENT_INSTANCE_1>	
    rr:logicalTable [ rr:sqlQuery """
    SELECT
	    PMS_SCYW.T_SB_ZNYC_PDBYQ.OBJ_ID ,
	(
		CASE PMS_SCYW.T_SB_ZNYC_PDBYQ.YXZT
		WHEN CAST ('31' AS NVARCHAR2(2)) THEN
			'部署状态(现场留用)'
		WHEN CAST ('10' AS NVARCHAR2(2)) THEN
			'部署状态(未投运)'
		WHEN CAST ('20' AS NVARCHAR2(2)) THEN
			'部署状态(在运)'
		END
	) STATE 
	
		FROM PMS_SCYW.T_SB_ZNYC_PDBYQ WHERE YXZT='31' OR YXZT='10' OR YXZT='20' """ ];
	rr:subjectMap [
		rr:template "http://nari.com/CDPSM/CIM_DEMO#{OBJ_ID}_AssetDeployment";
		rr:class cim:AssetDeployment;
		rr:termType rr:IRI;
	
	];
	
	
	rr:predicateObjectMap [
		rr:predicate cim:AssetDeployment.deploymentState;
		rr:objectMap [ 
			rr:template "http://nari.com/CDPSM/CIM_DEMO/T_SB_ZNYC_PDBYQ(YXZT)#{STATE}";
			rr:termType rr:Literal;
		];
	].
	

