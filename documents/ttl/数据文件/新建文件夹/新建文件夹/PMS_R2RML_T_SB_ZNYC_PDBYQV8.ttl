<#T_SB_ZNYC_PDBYQ_Map_Location_Asset>
	rr:logicalTable [ rr:sqlQuery """
    SELECT
	SSDS , SSDSMC,
	(
		CASE PMS_SCYW.T_SB_ZNYC_PDBYQ.DQTZ
		WHEN CAST ('01' AS NVARCHAR2(2)) THEN
			'地区特征(市中心区)'
		WHEN CAST ('02' AS NVARCHAR2(2)) THEN
			'机构类别(市区)'
		WHEN CAST ('03' AS NVARCHAR2(2)) THEN
			'机构类别(城镇)'
		WHEN CAST ('04' AS NVARCHAR2(2)) THEN
			'机构类别(县城)'	
		WHEN CAST ('05' AS NVARCHAR2(2)) THEN
			'机构类别(农村)'
		WHEN CAST ('06' AS NVARCHAR2(2)) THEN
			'机构类别(乡镇)'	
		WHEN CAST ('06' AS NVARCHAR2(2)) THEN
			'机构类别(农牧区)'	
		END
	) TYPE
	FROM PMS_SCYW.T_SB_ZNYC_PDBYQ 
    WHERE T_SB_ZNYC_PDBYQ.SSDS IS NOT NULL AND T_SB_ZNYC_PDBYQ.SSDSMC IS NOT NULL	""" ];
	rr:subjectMap [
		rr:template "http://nari.com/CDPSM/CIM_DEMO/T_SB_ZNYC_PDBYQ(SSDS)#{SSDS}";
		rr:class cim:Location;
		rr:termType rr:IRI;
	];
	
	rr:predicateObjectMap [
		rr:predicate cim:Location.type;
		rr:objectMap [
			rr:template "http://nari.com/CDPSM/CIM_DEMO/T_SB_ZNYC_PDBYQ(DQTZ)#{TYPE}";
			rr:termType rr:Literal;
		];
	];
	
	
	rr:predicateObjectMap [
